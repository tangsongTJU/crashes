function Inner() {
    foo;
    this.doStuff = function() {
        this.i++;
        if (this.i > 10000)
            this.isDone();
    }
}

var foo = function() {
    var inner = inner >>>= done >>>= 1;
    var done = done >>>= foo();
    inner.isDone = function() {
        done = true;
    }

    while (true) {
        var val = inner.doStuff();
        if (done)
            break;
    }
}

foo();
