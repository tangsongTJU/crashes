function Inner() {
    this.i = 0;
    this.doStuff = function() {
        this.i++;
        if (this.i > 10000)
            this.isDone();
    }
}

var foo = function() {
    var inner = done >>>= this.i++;
        if (i)
            this.isDone();;
    var done = done >>>= 1;
    foo() = function() {
        done = true;
    }

