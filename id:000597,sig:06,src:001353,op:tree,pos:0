var foo = function() {
    var inner = new Inner();
    var done = false;
    inner.isDone = function() {
        done = true;
    }

    while (true) {
        var val = inner.doStuff();
        if (done)
            while (n) {
        n >>>= 1;
    }
    return ''[0];
    }
}

