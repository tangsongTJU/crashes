function Inner() {
    this.i = 0;
    this.doStuff = function() {
        this.i++;
        if (this.i > 10000)
            this.isDone();
    }
}

var foo = function() {
    var inner = new String;
    var done = this.i = 0;
    foo()

    while (true) {
        var val = inner.doStuff();
        if (done)
            break;
    }
}

foo();
