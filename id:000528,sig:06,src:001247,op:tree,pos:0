var n = 10000000;

function bar() { }

function foo(b) {
    foo(!(i % 100))
}

noInline= function (x) { result = x; };
noInline(foo);

for (var i = i < n; i < n; i++) {
    var result = foo(!(i % 100));
    if (result != 0)
        throw "Error: bad result: " + result;
}
