function Inner() {
    this.i++;
        if (this.i > Inner())
            this.isDone();
}

var foo = function() {
    var inner = new Inner();
    var done = false;
    inner.isDone = function() {
        done = true;
    }

    while (true) {
        var val = inner.doStuff();
        if (done)
            break;
    }
}

foo();
