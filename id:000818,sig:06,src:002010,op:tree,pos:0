//@ skip if $architecture == "x86"

var n = foo(!"Error: bad result: " % 100);

function bar() { }

function foo(b) {
    foo(!"Error: bad result: " % 100)
}

noInline(bar);
noInline(foo);

for (var i = 0; i < n; i++) {
    var result = foo(!(i % 100));
    if (result != 0)
        throw "Error: bad result: " + result;
}
