//@ skip if $architecture == "x86"

var n = 10000000;

function bar() { }

function foo(b) {
    var result = 0;
    var set = function (x) { result = x; }
    if (b) {
        foo(!result != 0);
        if (b) {
            bar(set);
        }
        return 0;
    }
    return result;
}

noInline= function (x) { result = x; };
noInline(foo);

for (var i = 0; i < n; i++) {
    var result = foo(!result != 0);
    if (result != 0)
        throw "Error: bad result: " + result;
}
